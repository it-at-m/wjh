{
  "name": "lostopf",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:analyze": "vite build --mode analyze",
    "build:clean": "rimraf dist",
    "build:with-coverage": "cross-env VITE_COVERAGE=true npm run build",
    "preview": "vite preview",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "coverage:ci": "vitest run --coverage --reporter=junit --outputFile.junit=coverage/junit.xml",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:dev-open": "concurrently --kill-others --names server,cypress --success command-cypress \"cross-env VITE_COVERAGE=true npm run dev\" \"npm run cypress:open\"",
    "cypress:run-ci": "concurrently --kill-others --names server,cypress --success command-cypress \"cross-env VITE_COVERAGE=true npm run preview\" \"npm run cypress:run\""
  },
  "dependencies": {
    "@fontsource/roboto": "^5.0.2",
    "@mdi/font": "^7.2.96",
    "@tanstack/vue-query": "^4.29.11",
    "pinia": "^2.1.4",
    "uuid": "^9.0.0",
    "vue": "^3.3.4",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.4",
    "vuetify": "^3.3.12"
  },
  "devDependencies": {
    "@babel/types": "^7.21.4",
    "@cypress/code-coverage": "^3.10.7",
    "@intlify/unplugin-vue-i18n": "^0.10.0",
    "@pinia/testing": "^0.1.2",
    "@rushstack/eslint-patch": "^1.3.2",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.17.5",
    "@types/uuid": "^9.0.1",
    "@types/webfontloader": "^1.6.35",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/coverage-v8": "^0.32.0",
    "@vitest/ui": "^0.32.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "cross-env": "^7.0.3",
    "browserslist-to-esbuild": "^1.2.0",
    "concurrently": "^8.2.0",
    "cypress": "12.14.0",
    "cypress-multi-reporters": "^1.6.3",
    "cypress-vite": "^1.4.2",
    "eslint": "^8.45.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-vue": "^9.15.1",
    "eslint-plugin-vuetify": "^2.0.5",
    "jsdom": "^22.1.0",
    "mocha-junit-reporter": "^2.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "start-server-and-test": "^2.0.0",
    "typescript": "~5.1.6",
    "vite": "^4.4.6",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-istanbul": "^5.0.0",
    "vite-plugin-vuetify": "^1.0.2",
    "vitest": "^0.33.0",
    "vue-tsc": "^1.8.6"
  }
}